Public  class Main
{
 Public static void main (string[]args)
}
System.Out.println ("sum of odd " +sumOfOdd (0,100,0);
System.Out.println ("sum of even " +sumOfeven (0,100,0);
}
public static int sumOfOdd(int start, int end, int sum){
    return start>end ? sum : sumOfOdd(start+1,end,sum + (start%2!=0 ? start : 0));
 }
public static int sumOfeven(int start, int end, int sum){
    return start>end ? sum : sumOfeven(start+1,end,sum + (start%2==0 ? start : 0));
 }
}
